{
    "name": "updevru/php-esputnik-api",
    "description": "Library for e-sputnik.com REST api",
    "type": "library",
    "keywords": ["esputnik", "e-sputnik.com", "api"],
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "Andrey Mazuryan",
            "email": "a.mazuryan@gmail.com"
        },
        {
            "name": "Ladygin Sergey",
            "email": "sladygin@updev.ru"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpspec/phpspec": "dev-main"
    },
    "autoload": {
        "psr-4": { "Esputnik\\": "lib/Esputnik/" }
    }
}
